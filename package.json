  "bugs": {
    "url": "https://github.com/habx/lib-client-intl/issues"
  },
  "homepage": "https://github.com/habx/lib-client-intl#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "react": "^17.0.2"
  },
  "dependencies": {
